system "mixup"

root
    MIXUP: make

default
    assertion(boost)
    debug(no)
    trace(no)
    no_style_warning (no)
    no_warning(no)
    verbose(yes)
    manifest_string_trace(no)
    high_memory_compiler(yes)
    profile(no)
    relax(yes)

cluster
    liberty: "${path_liberty}/src/loadpath.se"
    main: "./loadpath.se"

generate
    no_strip(yes)
    clean(no)
    c_compiler_options: "-g -pipe -Os"
    split("by_type")

end
