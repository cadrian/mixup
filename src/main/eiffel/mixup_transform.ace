system "transform"

root
    MIXUP_TRANSFORM: make

default
    assertion(ensure)
    assertion_flat_check(yes)
    debug(no)
    trace(yes)

cluster
    mixup: "./loadpath.se"

    standard: "${path_liberty_core}loadpath.se"
        default
            assertion(no)
        option
            debug(yes): FIXED_STRING, PARTIALLY_FILLED_STRING
        end

end
